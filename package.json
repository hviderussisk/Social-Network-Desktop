{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Starter kit for creating React/Redux and Express apps",
  "main": "server.js",
  "engines": {
    "node": "12.18.3"
  },
  "scripts": {
    "client-install": "npm install --prefix social",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix social",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix social  && npm run build --prefix social"
  },
  "author": "Myshko Ilya",
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "bcrypt": "^5.0.0",
    "concurrently": "^4.1.2",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.0",
    "node-sass": "^4.14.1",
    "nodemon": "^1.19.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "socket.io": "^3.0.5"
  }
}
